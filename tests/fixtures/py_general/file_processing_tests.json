{
  "test_id": 3,
  "test_name": "File Processing Test",
  "description": "Tests file operations and text processing",
  "test_type": "py_general",
  "checks": [
    {
      "check_id": 1,
      "name_for_output": "Create File Processor",
      "reason_for_output": "Failed to create FileProcessor instance",
      "explain_for_error": "FileProcessor class should be properly instantiated",
      "spec": {
        "perform": {
          "action": "create_object",
          "target": "FileProcessor",
          "params": {
            "args": ["./temp"]
          },
          "save_as": "processor"
        },
        "expect": {
          "return_value": {
            "assertion": "is_instance_of",
            "value": "FileProcessor"
          }
        }
      }
    },
    {
      "check_id": 2,
      "name_for_output": "Create Test File",
      "reason_for_output": "File creation failed",
      "explain_for_error": "create_file method should create a file and return its path",
      "spec": {
        "perform": {
          "action": "call_method",
          "target": "create_file",
          "params": {
            "object_ref": "processor",
            "args": ["test.txt", "Hello World\nThis is a test file\nWith multiple lines"]
          },
          "save_as": "file_path"
        },
        "expect": {
          "return_value": {
            "assertion": "contains",
            "value": "test.txt"
          }
        }
      }
    },
    {
      "check_id": 3,
      "name_for_output": "Count Lines",
      "reason_for_output": "Line count is {actual}, expected {expected}",
      "explain_for_error": "count_lines method should return the correct number of lines",
      "spec": {
        "perform": {
          "action": "call_method",
          "target": "count_lines",
          "params": {
            "object_ref": "processor",
            "args": ["test.txt"]
          }
        },
        "expect": {
          "return_value": {
            "assertion": "equals",
            "value": 3
          }
        }
      }
    },
    {
      "check_id": 4,
      "name_for_output": "Count Words",
      "reason_for_output": "Word count is {actual}, expected {expected}",
      "explain_for_error": "count_words method should return the correct number of words",
      "spec": {
        "perform": {
          "action": "call_method",
          "target": "count_words",
          "params": {
            "object_ref": "processor",
            "args": ["test.txt"]
          }
        },
        "expect": {
          "return_value": {
            "assertion": "is_in_range",
            "value": {"min": 8, "max": 12}
          }
        }
      }
    }
  ]
}